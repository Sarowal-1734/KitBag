            <enum name="VOIP" value="6"/>
            <enum name="PERSONAL_NUMBER" value="7"/>
            <enum name="PAGER" value="8"/>
            <enum name="UAN" value="9"/>
            <enum name="VOICEMAIL" value="10"/>
            <enum name="UNKNOWN" value="11"/>

        </attr>


        <!--CCP dimension properties-->
        <attr format="dimension" name="ccp_padding"/>

        <!--CCP bool properties-->
        <attr format="boolean" name="ccp_showNameCode"/>
        <attr format="boolean" name="ccp_showFlag"/>
        <attr format="boolean" name="ccp_showFullName"/>
	    <attr format="boolean" name="ccp_internationalFormattingOnly"/>
